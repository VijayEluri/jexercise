<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated junit-exercise -->
      <package
            uri="platform:/plugin/no.hal.learning.exercise.junit/model/junit-exercise.ecore"
            class="no.hal.learning.exercise.junit.JunitPackage"
            genModel="model/junit-exercise.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="platform:/plugin/no.hal.learning.exercise.junit/model/junit-exercise.ecore"
            class="no.hal.learning.exercise.junit.adapter.JunitTaskEventsAdapterFactory"
            supportedTypes=
              "no.hal.learning.exercise.TaskEventsAdapter"/>
   </extension>
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="platform:/plugin/no.hal.learning.exercise.junit/model/junit-exercise.ecore"
            class="no.hal.learning.exercise.junit.adapter.JunitUIAdapterFactory"
            supportedTypes=
              "no.hal.emf.ui.parts.adapters.EObjectUIAdapter"/>
   </extension>

</plugin>
